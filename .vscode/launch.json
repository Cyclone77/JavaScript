    "version": "0.2.0",
    "configurations": [{
        "type": "node",
        "request": "launch",
        "runtimeArgs": [
            "--debug",
            "--nolazy"
        ],
        //"trace": "all",
        "stopOnEntry": true,
        "name": "JavaScript Debug",
        "program": "${file}",
        "cwd": "${workspaceRoot}",
        "console": "internalConsole"
    }]
}